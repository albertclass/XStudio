{
    "version": "0.2.0",
    "configurations": [{
            "name": "gate_server Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/bin/Debug/gate_server",
            "args": ["-debug", "../gate-server.ini"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
